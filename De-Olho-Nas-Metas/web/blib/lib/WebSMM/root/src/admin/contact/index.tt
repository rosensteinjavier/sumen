<div>
<div class="rows">
<h2 class="sub-header">Mensagens recebidas no Fale Conosco</h2>
<div class="table-responsive">
	[%IF contacts.size > 0%]
	<table class="table table-striped">
		<thead>
			<tr>
                <th>Nome</th>
                <th>E-mail</th>
                <th>Mensagem</th>
                <th>Data</th>
			</tr>
		</thead>
		<tbody>
			[%FOREACH ct IN contacts%]
				<tr>
					<td>[%ct.name%]</td>
					<td>[%ct.email%]</td>
					<td>[%ct.message%]</td>
					<td>[%ct.created_at%]</td>
				</tr>
            [%END%]
		</tbody>
	</table>
	[%ELSE%]
		<h4>Nenhum usu√°rio cadastrado</h4>
	[%END%]
		

</div>
</div>
</div>
